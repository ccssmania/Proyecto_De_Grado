extends ../../layout.jade
block contenido
	div(class="container")
		h1 MIS IMAGENES
		for imagen in imagenes
			.white.border.tr.large-padding
				.row
					.col-md-10
						h2= imagen.title
					.col-md-2
						form(action="/app/imagenes/#{imagen.id}?_method=DELETE" method="POST",class="link")
							input(type="submit", value="Eliminar")
